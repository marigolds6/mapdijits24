.mapdijitsResultsContent {
	cursor:default;
}
.mapdijitsResultsContent div {
	text-align: left;
}
.resultsPaneNoResult {
    margin: 2px 2px 2px 0px;
    padding: 4px 4px 4px 32px;
	color: #808080;
}
.resultsPaneNoMatch {
	color: #000000;
	list-style-type: none;
}
.resultsPaneBack {
	text-decoration: underline;
	cursor: pointer;
}
.resultsItem {
    background-color: #D4D4D4;
    margin: 2px 2px 2px 0px;
    padding: 4px 4px 4px 32px;
    border: 0;
    font-size: 8pt;
    font-weight: normal;
    -o-border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
	background-repeat: no-repeat;
	background-position: left center;
	cursor: pointer;
}
div.resultsItem:hover {
	background-color: #C9DDF8;
}
.resultsItem.alt {
    background-color: #999999;
}
.resultsContainer.hide {
	display: none;
}
.resultsContainer.hideAll {
	display: none !important;
}
.resultsContainer.showAll {
	display: block !important;
}

.resultsPaneHighlight{
	background: #6699CC none repeat scroll 0 0 !important;
	color: #000000;
}
.resultsPaneSelect{
	background: #0080FF none repeat scroll 0 0 !important;
	color: #FFFFFF;
}
.resultsItemTitle {
	font-weight: bold;
}
.resultsItemContent {
	
}
.resultsItemNav {
    background-color: #C9DDF8;
    margin: 2px 2px 2px 0px;
    padding: 4px;
    border: 0;
    font-size: 12pt;
    font-weight: bold;
    -o-border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
	padding-left: 32px;
	background-repeat: no-repeat;
	background-position: left center;
	position: relative;
}
.resultsItemNext {
    background-color: #6699CC;
    padding: 4px;
    border: 0;
    -o-border-radius: 3px;
    -moz-border-radius-topright: 3px;
    -moz-border-radius-bottomright: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
	cursor: pointer;
}
.resultsItemPrev {
    background-color: #6699CC;
    padding: 4px;
    border: 0;
    -o-border-radius: 3px;
    -moz-border-radius-topleft: 3px;
    -moz-border-radius-bottomleft: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
	cursor: pointer;	
}
.resultsItemNavDisabled {
	color: #999999;
    background-color: #A8C2E4;
    padding: 4px;
	border: 0;
    -o-border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
	cursor: default;
}
